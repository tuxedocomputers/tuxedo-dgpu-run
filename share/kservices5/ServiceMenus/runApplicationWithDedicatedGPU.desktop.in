[Desktop Entry]
Type=Service
ServiceTypes=KonqPopupMenu/Plugin
MimeType=application/x-desktop
Actions=runApplicationWithDedicatedGPU
X-KDE-Priority=TopLevel

[Desktop Action runApplicationWithDedicatedGPU]
Name=Run Application with dedicated GPU
Exec=${CMAKE_INSTALL_PREFIX}/libexec/dgpu-run-desktop-entry %u
